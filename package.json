    "@exeto/babel-preset-react": "^0.2.1",
    "@exeto/eslint-config": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "commitizen": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1"
